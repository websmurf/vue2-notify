    "bootstrap": "^3.3.7|^4.0.0",
    "velocity-animate": "^1.5.0",
    "vue": "^2.5.11"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.15",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
